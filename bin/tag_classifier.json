    "device_type": {
        "regex": "_(TT|TI|PT|PI|F(|Q)T|FI|LT|LI|SIREN|FAN|FPC|PMP|HTR|FCV|AGT|ISC|APF|CRU|CVR|FDR|HPP|SCR|WT|FTP|MASS)(|_)(\\d{2})",
        "on_match": {
            "device_type": {
                "group": 0,
                "value": {
                    "TT": "датчик температуры",
                    "TI": "датчик температуры",
                    "PT": "датчик давления",
                    "PI": "датчик давления",
                    "FT": "расходомер",
                    "FI": "расходомер",
                    "FQT": "счетчик расходомера",
                    "LT": "уровнемер",
                    "LI": "уровнемер",
                    "SIREN": "сирена",
                    "FAN": "вентилятор",
                    "FPC": "контроллер вентилятора",
                    "PMP": "насос",
                    "HTR": "подогреватель",
                    "FCV": "клапан",
                    "AGT": "агитатор",
                    "ISC": "перекачной насос",
                    "APF": "питатель пластинчатый",
                    "CRU": "дробилка",
                    "CVR": "конвейер",
                    "FDR": "вибропитатель",
                    "HPP": "hudraulic power pack",
                    "SCR": "conveyer scrubber",
                    "WT": "вес",
                    "FTP": "Фильтр-пресс",
                    "MASS": "масса"
                }
            },
            "device_type_id": {
                "group": 0,
                "value": ""
            },
            "device_num": {
                "group": 3,
                "value": ""
            }
        }
    },
    "alarm": {
        "regex": "(_(AH|AHH|AL_|ALL|ALARM)_)",
        "on_match": {
            "tag_type": {
                "group": -1,
                "value": "alarm"
            },
            "tag_descr": {
                "group": 1,
                "value": {
                    "AH": "высокий уровень",
                    "AHH": "критически высокий уровень",
                    "AL": "низкий уровень",
                    "ALL": "критически низкий уровень",
                    "ALARM": "тревога",
                    "ALM": "тревога"
                }
            }
        }
    },
    "alarm_value": {
        "regex": "(_(HI|HIHI|LO|LOLO)_)",
        "on_match": {
            "tag_type": {
                "group": -1,
                "value": "alarm"
            },
            "tag_descr": {
                "group": 1,
                "value": {
                    "HI": "высокий уровень - значение",
                    "HIHI": "критически высокий уровень - значение",
                    "LO": "низкий уровень - значение",
                    "LOLO": "критически низкий уровень - значение"
                }
            }
        }
    },
    "state": {
        "regex": "(_(URS|UMH|SAS|SST|SSP|DQS|SLR|DFST|USH|DIR|SDI|HR|DMR|DOF|XY|RST|ET|PR)_)",
        "on_match": {
            "tag_type": {
                "group": -1,
                "value": "state"
            },
            "tag_descr": {
                "group": 1,
                "value": {
                    "URS": "в работе",
                    "UMH": "MCC статус",
                    "SAS": "авто/мануал",
                    "SST": "scada старт",
                    "SSP": "scada стоп",
                    "DQS": "drive sequence start",
                    "SLR": "локал/ремоут",
                    "DFST": "старт по месту",
                    "USH": "стоп по месту",
                    "DIR": "отсутствие блокировок",
                    "SDI": "блокировки отключены",
                    "HR": "сброс моточасов",
                    "DMR": "готовность",
                    "DOF": "ошибка запуска",
                    "XY": "команда запуска",
                    "RST": "сброс",
                    "ET": "время ожидания",
                    "PR": "шаг"
                }
            }
        }
    },
    "motohours": {
        "regex": "(_(DRH|DRM)_)",
        "on_match": {
            "tag_descr": {
                "group": 1,
                "value": {
                    "DRH": "часы",
                    "DRM": "минуты"
                }
            }
        }
    },
    "step": {
        "regex": "S(\\d{1,2})",
        "on_match": {
            "proc_step": {
                "group": 0,
                "step": {
                    "group": 1,
                    "value": ""
                }
            }
        }
    }
}