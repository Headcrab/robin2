                "group": 1,
                "value": ""
            },
            "area_descr": {
                "group": 1,
                "value": {
                    "10": "Дробление",
                    "11": "Тоннель золотой цепочки",
                    "15": "Тоннель медной цепочки",
                    "20": "Измельчение золотой цепочки",
                    "25": "Измельчение медной цепочки",
                    "30": "Trash screening, CIP",
                    "31": "Регенерация",
                    "32": "Детоксикация",
                    "35": "Флотация",
                    "36": "Очистка флотации",
                    "37": "Перечистка флотации",
                    "40": "Acid wash",
                    "45": "Элюация",
                    "50": "Goldroom",
                    "55": "Сгущение",
                    "70": "Water dist",
                    "71": "Fire water",
                    "80": "Цианирование",
                    "81": "Air service",
                    "85": "Флокулянт"
                }
            }
        }
